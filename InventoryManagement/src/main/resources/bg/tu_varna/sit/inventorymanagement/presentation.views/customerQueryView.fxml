                <Font name="System Bold Italic" size="12.0" />
            </font>
        </Label>
      <DatePicker fx:id="fromDate" layoutX="35.0" layoutY="34.0" onAction="#getFromDate" />
      <DatePicker fx:id="toDate" layoutX="248.0" layoutY="34.0" onAction="#getToDate" />
